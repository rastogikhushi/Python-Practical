#2. Function to count no. vowels and consonants
def count(x):
    c=0 
    v=0
    for ch in x.lower():
        if ch in 'aeiou':
            v+=1
        elif ch.isalpha():
            c+=1
    return v,c
st = input("enter string ")
v,c = count(st)
print("no of vowels",v)
print("no of consonants",c)
